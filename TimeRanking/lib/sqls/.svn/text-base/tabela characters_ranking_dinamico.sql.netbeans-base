DROP TABLE IF EXISTS ranking_dinamico;
CREATE TABLE ranking_dinamico (
  char_id INTEGER NOT NULL,
  pvp_diario INTEGER NOT NULL,
  pvp_semanal INTEGER NOT NULL,
  pvp_mensal INTEGER NOT NULL,
  pk_diario INTEGER NOT NULL,
  pk_semanal INTEGER NOT NULL,
  pk_mensal INTEGER NOT NULL,
  PRIMARY KEY (char_id)
);